    "import turtle\n",
    "t=turtle.Turtle()\n",
    "d=0.1\n",
    "while True:\n",
    "    t.forward(d)\n",
    "    t.left(10)\n",
    "    d+=0.1\n",
    "    if t.distance (0,0)>=100:\n",
    "        break\n",
    "turtle.done()"
   ]
